# Private key from your wallet (without 0x prefix)
# Get it from MetaMask: Account Details -> Export Private Key
PRIVATE_KEY=your_private_key_here

# RPC URL for Sepolia testnet
# Option 1: Infura (https://infura.io) - Sign up and create a project
# Option 2: Alchemy (https://alchemy.com) - Sign up and create an app
# Option 3: Public RPC (not recommended for production)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY

# Etherscan API key for contract verification
# Get it from https://etherscan.io/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key

# Gas reporter
REPORT_GAS=false
